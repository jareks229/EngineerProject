@model ProjektInzynier.Models.OrderModel


<br/>
<br/>
<div align="center">
    <h2><i class="fas fa-check-square"></i> Finalizowanie dokumentacji</h2>

    <p> Proszę podać dane firmy aby sfinalizować dokumentację</p>

    <form asp-action="Create" asp-controller="Excel" method="post">
        <h3>Inżynier</h3>
        <div class="form-group">
            <label><i class="fas fa-file-signature"></i> Imię i Nazwisko</label><input asp-for="Name" class="form-control" />
        </div>
        <h3>Adres firmy:</h3>
        <div class="form-group">
            <label>Wiersz 1:</label><input asp-for="Line1" class="form-control" />
        </div>
        <div class="form-group">
            <label>Wiersz 2:</label><input asp-for="Line2" class="form-control" />
        </div>
        <div class="form-group">
            <label>Wiersz 3:</label><input asp-for="Line3" class="form-control" />
        </div>
        <div class="form-group">
            <label>Miasto:</label><input asp-for="City" class="form-control" />
        </div>
        <div class="form-group">
            <label>Województwo</label><input asp-for="State" class="form-control" />
        </div>
        <div class="form-group">
            <label>Kod Pocztowy:</label><input asp-for="ZipCode" class="form-control" />
        </div>
        <div class="form-group">
            <label>Kraj</label><input asp-for="Country" class="form-control" />
        </div>
        <div class="text-center">
            <button type="submit" class="btn btn-success"><i class="far fa-file"></i> Wykonaj dokumentację</button>
        </div>

    </form>
</div>

